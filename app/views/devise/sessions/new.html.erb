<div class="row">
  <div class="col m6 offset-m3  center">


    <h5>Entrar</h5>

    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
       <%= render 'shared/devisemes' %>


      <div class="input-field">
        <%= f.email_field :email , :class=>"validate" , :placeholder=> "Email" %>
      </div>

      <div class="input-field">
         <%= f.password_field :password, autocomplete: "off", :class=>"validate" , :placeholder=> "Senha"  %>
      </div>

      <button class="btn waves-effect waves-light" type="submit" name="action" > Entrar </button>
       <%= link_to "Esqueci minha senha" , new_user_password_path, :class=> "btn waves-effect waves-light" %>
      <% end %>

  </div>
</div>




